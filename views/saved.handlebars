<nav id="nav" class="navbar navbar-dark bg-dark">
  <!-- Navbar content -->
  <nav class="navbar-brand">
    <button class="btn btn-outline-success" type="button">Home</button>
    <button class="btn btn-outline-info" type="button">Saved Articles</button>
  </nav>
  <button class="btn btn-outline-warning" type="button">Scrape NEW Articles!</button>
</nav>
<section id="title">
  <div class="container">
    <h1>Saved Articles<img src="images/ball.jpg" height="35" width="35"></h1>
  </div>
</section>
<section class="section">
  <div class="container">
    {{#if data}}
    {{#each data}}
    {{> stored}}
    {{/each}}
    {{else}}
    <h1>No Articles Saved</h1>
    {{/if}}
  </div>
</section>

<div class="modal fade animated rollIn" id="myModal" tabindex="-1" role="dialog" aria-labelledby="exampleModalLabel"
  aria-hidden="true">
  <div class="modal-dialog" role="document">
    <div class="modal-content">
      <div class="modal-header">
        <div class="modal-title"></div>
        <button type="button" class="close" data-dismiss="modal" aria-label="Close">
          <span aria-hidden="true">&times;</span>
        </button>
      </div>
      <div class="modal-body">
        
       </div>
       
      <div class="modal-footer">
        <button type="button" class="btn btn-info" data-dismiss="modal">Close</button>
      </div>
    </div>
  </div>
</div>